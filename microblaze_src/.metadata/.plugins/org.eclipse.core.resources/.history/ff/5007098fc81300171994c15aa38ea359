#ifndef TIME_H_
#define TIME_H_

void timerTick(void* ref) {
  ct++; // increase ct every millisecond
}

void delay(u32 ms) {
  ct = 0; // set the counter to 0
  while (ct < ms) // wait for ms number of milliseconds
    ;
}

#endif /* TIME_H_ */
